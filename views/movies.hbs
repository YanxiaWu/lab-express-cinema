
<div class="container">
    <div class="picktitle">
       <h2>Ironhack Cinema</h2>
       <h4>Clinck on the movie to check the showtimes!</h4>
        <a href="/">Back to HOME page</a>
    </div>


    <div class="row">
        {{#each movies}}
        <div class="col-4">
            <img src={{image}} class="card-img-top img" alt="image">
            <h6 class="card-title">{{title}}</h6>
            <a href="/movies/{{_id}}">See more</a>
        </div>
        {{/each}}
    </div>
</div>

   
